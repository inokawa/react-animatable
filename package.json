    "@storybook/test-runner": "0.7.2",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/styled-components": "5.1.26",
    "@vanilla-extract/css": "1.9.2",
    "@vanilla-extract/webpack-plugin": "2.2.0",
